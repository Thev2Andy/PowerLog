<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>net7.0</TargetFramework>
    <RootNamespace>PowerLog</RootNamespace>
	  
    <AssemblyVersion>1.2.1</AssemblyVersion>
    <FileVersion>1.2.1</FileVersion>
    <Authors>Thev2Andy</Authors>
    <PackageProjectUrl>https://thev2andy.github.io/PowerLog/</PackageProjectUrl>
    <RepositoryUrl>https://github.com/Thev2Andy/PowerLog</RepositoryUrl>
    <Description>PowerLog is a modular, dependency-free, lightweight .NET 7 logging library made in C#, that is just built different.</Description>
    <PackageIcon>PowerLog Icon.png</PackageIcon>
    <PackageIconUrl />
    <Version>1.2.1</Version>
	<GenerateAssemblyInfo>False</GenerateAssemblyInfo>
	<GeneratePackageOnBuild>True</GeneratePackageOnBuild>
	<PackageTags>Log, Logger, PowerLog, Power, Sharp, CSharp, C#, Modular, Library, Lib, Lightweight</PackageTags>
	<PackageLicenseExpression>Apache-2.0</PackageLicenseExpression>
	  
	<Nullable>disable</Nullable>
  </PropertyGroup>

  <PropertyGroup Condition="'$(Configuration)|$(Platform)'=='Release|AnyCPU'">
    <DocumentationFile>..\PowerLog\PowerLog\XML Docs\PowerLog XML.xml</DocumentationFile>
  </PropertyGroup>

  <ItemGroup>
    <None Include="Resources\Icon\PowerLog Icon.png">
      <Pack>True</Pack>
      <PackagePath></PackagePath>
    </None>
  </ItemGroup>

</Project>